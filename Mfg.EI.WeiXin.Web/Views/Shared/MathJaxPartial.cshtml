
<script src="@Mfg.EI.WeiXin.WeiXinConfig.JsUrl/Common/MathJax-master/MathJax.js?config=TeX-AMS-MML_HTMLorMML"></script>
<script type="text/javascript">
    var mathjaxDivName = "@ViewData["mathJaxMainDivName"]";
    $(function () {
        if (mathjaxDivName) {
            InitMathJax(mathjaxDivName);
        }
    });
    function InitMathJax(div) {
        //MathJax.Hub.Queue(["Typeset", MathJax.Hub, document.getElementById("contentIframe")]);

        MathJax.Hub.Queue(["Typeset", MathJax.Hub, document.getElementById(div)]);
    }
</script>
